function widgets_PopUpImage(n,t,i,r){t=t.replace(/#APOSTROPHE#/g,"'");var u=new Image;u.src=n;u.onload=function(){GetParent().PopUpImage(n,t,u.width,u.height,i);r&&$(window).trigger(r)}}function widgets_scrollToElement(n,t){GetParent().scrollToElement(t,n)}function widgets_linkToUrl(n){window.open(n,"_blank")}function openPage(n){window.location.href=n}function widgets_linkToPage(n){GetParent().openPage(n)}function GetParent(){return parent&&parent.ISSTUDIO?parent:window}function resizeend_magicWidget(){new Date-rtime_magicWidget<delta_magicWidget?setTimeout(resizeend_magicWidget,delta_magicWidget):(timeout_magicWidget=!1,updateMagicWidgetFullWidth())}function updateMagicWidgetFullWidth(){setTimeout(function(){$(".dataTypeMagicWidget.fit_to_bg_new").each(function(){try{window["fixWidgetAccordingToMediaQuery_"+this.getAttribute("data-id")+"_ey"](this.getAttribute("data-id")+"_ey")}catch(n){}})},100)}function SetImageOptimization(n,t,i,r,u,f){try{toInt?(u=toInt(u),f=toInt(f)):(u=parseInt(u),f=parseInt(f));var e=400,l=e*i/t,o=512,a=o*i/t,s=650,v=s*i/t,h=850,y=h*i/t,c=1024,p=c*i/t;return e<=t&&e>=u&&l>=f?400:o<=t&&o>=u&&a>=f?512:s<=t&&s>=u&&v>=f?650:h<=t&&h>=u&&y>=f?850:c<=t&&c>=u&&p>=f?1024:-1}catch(w){return-1}}rtime_magicWidget=new Date;var timeout_magicWidget=!1,delta_magicWidget=200;$(window).on("windowResize",function(){rtime_magicWidget=new Date;timeout_magicWidget===!1&&(timeout_magicWidget=!0,setTimeout(resizeend_magicWidget,delta_magicWidget))})